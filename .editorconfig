root = true
[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
[Makefile]
indent_style = tab
charset = utf-8
[*.yml,yaml]
indent_style = space
indent_size = 4
charset = utf-8
[*.cs,*.csproj]
indent_style = space
indent_size = 4
charset = utf-8-bom
[*.sql]
indent_style = space
indent_size = 2
charset = utf-8
